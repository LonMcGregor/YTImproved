{
	"applications": {
		"gecko": {
			"id": "ytimproved@lonmcgregor"
		}
	},
	"manifest_version": 2,
	"name": "YT Improved Alpha",
	"version": "1.1",
	"icons": {
		"48": "icons/icon48.png",
		"96": "icons/icon96.png"
	},
	"content_scripts": [
	  {
		"matches": ["*://*.youtube.com/*"],
		"css": ["css/hidden.css", "css/colours.css", "css/watch.css", "css/masthead.css"],
		"js": ["scripts/contentScript.js"]
	  }
	],
	"web_accessible_resources": ["scripts/main.js"]
}